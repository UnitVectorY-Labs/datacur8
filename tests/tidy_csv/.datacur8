version: "0.0.0"
types:
  - name: product
    input: csv
    match:
      include:
        - "^data/.*\\.csv$"
    schema:
      type: object
      required: ["id", "name", "price"]
      properties:
        id: { type: string }
        name: { type: string }
        price: { type: number }
      additionalProperties: false
